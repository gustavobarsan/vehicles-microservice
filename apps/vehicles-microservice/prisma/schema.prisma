generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vehicle {
  id      String @id @default(uuid())
  plate   String @unique
  chassis String @unique
  renavam String @unique
  model   String
  brand   String
  year    Int
  color   String
}